PORT=3001
DEEPEVAL_URL=http://localhost:8000/eval

# LLM Provider Selection
# Options: 'groq' (default), 'openai', 'azure-openai'
LLM_PROVIDER=groq
# LLM_PROVIDER=azure-openai


# Model to use for evaluation
EVAL_MODEL=llama-3.3-70b-versatile 
# EVAL_MODEL=gpt-4.1

# ============================================
# Option 1: Groq Configuration (default)
# ============================================
GROQ_API_KEY=gyour-groq-api-key-here

# ============================================
# Option 2: OpenAI Configuration
# ============================================
# OPENAI_API_KEY=sk-your-openai-api-key-here
# EVAL_MODEL=gpt-4o-mini

# ============================================
# Option 3: Azure OpenAI Configuration
# ============================================
LLM_PROVIDER=azure-openai
AZURE_OPENAI_API_KEY=Your azure-openai-api-key-here
AZURE_OPENAI_API_ENDPOINT=your-azure-openai-endpoint
AZURE_OPENAI_DEPLOYMENT_NAME=your-deployment-name
AZURE_OPENAI_API_VERSION=you
